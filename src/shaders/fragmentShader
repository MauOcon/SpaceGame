#version 330 core

// Inputs to the fragment shader
in vec2 pass_textureCoords;

// Outputs of the fragment shader
out vec4 out_Color;

uniform sampler2D textureSampler;	// Represents the texture

void main(void){
	
	// Returns the color of the pixel on the texture ato the coordinates 
	out_Color = texture(textureSampler, pass_textureCoords);

}